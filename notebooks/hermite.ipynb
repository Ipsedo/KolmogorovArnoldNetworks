    "s = 10\n",
    "n = 20\n",
    "\n",
    "x = th.randn(s)\n",
    "h = th.ones(s, n + 1)\n",
    "\n",
    "h[:, 1] = x\n",
    "for i in range(1, n):\n",
    "    h[:, i + 1] = x * h[:, i] - i * h[:, i - 1]"
